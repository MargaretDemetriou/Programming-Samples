if(nudTendered.Value >= nudBill.Value)
{
  decimal changeToBeGiven = nudTendered.Value - nudBill.Value;
  decimal[] currencies = { 100, 50, 20, 10, 5, 2, 1, 0.5M, 0.2M, 0.1M, 0.05M, 0.02M, 0.01M};
  foreach (decimal currency in currencies)
  {
    while(changeToBeGiven >= currency)
    {
      changeToBeGiven -= currency;
      lstChange.Items.Add(currency);
    }
  }
}
else
{
  MessageBox.Show("Please give more money");
}
